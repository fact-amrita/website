generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserCredential {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  RegisterDate String?
  email        String  @unique
  image        String?
  name         String
  provider     String?
  role         String?
}

model ExistingMembersList {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String? @unique
  name  String?
}

model User {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  RegisterDate String
  email        String  @unique
  image        String?
  name         String
  role         String
  FactID       String  @unique
  domain       String
  phone        BigInt?
  birthday     String?
}
